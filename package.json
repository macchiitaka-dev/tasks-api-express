{
  "name": "sample-tasks-express",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/sample-tasks-express.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "build:tsc": "tsc --incremental false",
    "coverage": "run-p coverage:**",
    "coverage:jest": "jest --coverage --passWithNoTests",
    "fmt": "run-p fmt:**",
    "fmt:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --fix --cache",
    "fmt:json": "prettier --write '**/*.json'",
    "fmt:markdown": "prettier --write '**/*.md'",
    "fmt:package-json": "sort-package-json",
    "initialize": "run-p initialize:*",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --cache",
    "prisma:generate": "prisma generate",
    "start": "ts-node-dev src/server.ts",
    "test": "npm run test:watch",
    "type-watch": "tsc --noEmit -w",
    "test:once": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests"
  },
  "dependencies": {
    "@prisma/client": "2.13.1",
    "express": "^4.17.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@prisma/cli": "2.13.1",
    "@types/eslint": "^7.2.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/prettier": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@typescript-eslint/typescript-estree": "^4.9.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-unicorn": "25.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.48.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "12.20.0",
    "npm": "6.14.8",
    "yarn": "1.22.10"
  }
}
